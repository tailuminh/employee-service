{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "employee",
    "type": "object",
    "properties": {
      "citizenId": {
        "type": "string",
        "minLength": 2,
        "pattern": "^[^-\\s][0-9]{11}$"
      },
      "firstName": {
        "type": "string",
        "minLength": 2
      },
      "lastName": {
        "type": "string",
        "minLength": 2
      },
      "dob": {
        "type": "string"
      },
      "department": {
        "type": "string",
        "minimum": 2
      },
      "address": {
        "type": "string",
        "minimum": 2
      },
      "workingStatus": {
        "type": "string",
        "minimum": 2
      }
    },
    "required": [
      "citizenId",
      "firstName",
      "lastName",
      "dob",
      "department",
      "address",
      "workingStatus"
    ],
    "additionalProperties": false
  }
  